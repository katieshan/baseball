addup <- function(year, df0, df1, df2){
lag1<-year-1
lag2<-year-2
out <- df0 %>%
select(person.key, games_count, games_above_count, points_sum, points_mean, points_sd, team.key_mode, age, since_debut) %>%
mutate(year=year)
