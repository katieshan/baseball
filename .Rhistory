lag2 <- df2 %>%
select(-age, -since_debut) %>%
rename_at(vars(-person.key),function(x) paste0(x,"_lag2"))
df <- merge(out, lag1, by="person.key")
df <- merge(df, lag2, by="person.key")
return(df)
}
